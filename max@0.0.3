#include "stdafx.h"
#include <iostream>
#include <sstream>
using namespace std;

bool read_numbers(int array[10])
{
	string string;
	getline(cin, string);
	istringstream stream(string);
	bool success = true;
	for (int i = 0; i < 10; ++i) {
		if (!(stream >> array[i])) {
			success = false;
			break;
		}
	}

	return success;
}

int main()
{
	int max = 0;
	int array1[10];
	int array2[10];

	if (read_numbers(array1) && read_numbers(array2)) {
		for (int a = 0; a < 10; ++a) {
			for (int b = 0; b < 10; ++b) {
				if (((array1[a] + array2[b]) > max) && (a <= b)) {
					max = array1[a] + array2[b];
				}
			}
		}
		cout << max;
	}
	else {
		cout << "An error has occured while reading numbers from line";
	}
	return 0;
}
